      <label>Scenario for testing template with match 'app[count(descendant::rdg) = 1]</label>
      <input-wrap xmlns="">
         <x:context xmlns:x="http://www.jenitennison.com/xslt/xspec">
            <app>
               <rdgGrp n="['never']">
                  <rdg wit="fMS">never </rdg>
               </rdgGrp>
            </app>
            <app>
               <rdgGrp n="['never']">
                  <rdg wit="f1818">never </rdg>
                  <rdg wit="f1823">never </rdg>
                  <rdg wit="fThomas">never </rdg>
                  <rdg wit="f1831">never </rdg>
                  <rdg wit="fMS">&lt;lb n="c56-0103__main__19"/&gt;never </rdg>
               </rdgGrp>
            </app>
         </x:context>
      </input-wrap>
      <input-wrap xmlns="">
         <x:call xmlns:x="http://www.jenitennison.com/xslt/xspec" template="mergeLoner"/>
      </input-wrap>
      <result select="/element()">
         <content-wrap xmlns="">
            <app xmlns:cx="http://interedition.eu/collatex/ns/1.0" xmlns:fv="http://fv">
               <rdgGrp n="['never']">
                  <rdg xmlns:x="http://www.jenitennison.com/xslt/xspec" wit="f1818">never </rdg>
                  <rdg xmlns:x="http://www.jenitennison.com/xslt/xspec" wit="f1823">never </rdg>
                  <rdg xmlns:x="http://www.jenitennison.com/xslt/xspec" wit="fThomas">never </rdg>
                  <rdg xmlns:x="http://www.jenitennison.com/xslt/xspec" wit="f1831">never </rdg>
               </rdgGrp>
               <rdgGrp n="['never', 'never']">
                  <rdg wit="fMS">never &lt;lb n="c56-0103__main__19"/&gt;never </rdg>
               </rdgGrp>
            </app>
         </content-wrap>
      </result>
      <test id="x645b9013-0c8d-3c92-ae53-9216f2a60443-expect1" successful="true">
         <label>merge the loner rdg into the next app</label>
         <expect select="/element()">
            <content-wrap xmlns="">
               <app xmlns:x="http://www.jenitennison.com/xslt/xspec">
                  <rdgGrp n="['never']">
                     <rdg wit="f1818">never </rdg>
                     <rdg wit="f1823">never </rdg>
                     <rdg wit="fThomas">never </rdg>
                     <rdg wit="f1831">never </rdg>
                  </rdgGrp>
                  <rdgGrp n="['never', 'never']">
                     <rdg wit="fMS">never &lt;lb n="c56-0103__main__19"/&gt;never </rdg>
                  </rdgGrp>
               </app>
            </content-wrap>
         </expect>
      </test>
