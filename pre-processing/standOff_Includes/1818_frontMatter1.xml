    <!--rm: centered ornamental -->
    
    <head>IN THREE VOLUMES.</head>
    <!--rm: centered ornamental -->
    <epigraph>
        <cit>
            <quote><lg>
                <l>Did I request thee, Maker, from my clay</l>
                <l>To mould me man? Did I solicit thee</l>
                <l>From darkness to promote me?—</l>
            </lg></quote>
            <bibl>P<hi rend="smallcaps">ARADISE</hi> L<hi rend="smallcaps">OST</hi>.</bibl>
